<% include partials/header %>

<h1>The Post Pages</h1>

<% for(var i = 0; i < posts.length; i++){ %>
    <li>
       <%= posts[i].tittle %> - <strong> <%= posts[i].author %> </strong>
    </li>
<% } %>

<% posts.forEach(function(post){ %>
    <li>
       <%= post.tittle %> - <strong> <%= post.author %> </strong>
    </li>
<% }) %>

<% include partials/footer %>